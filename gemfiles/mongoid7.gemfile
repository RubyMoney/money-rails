source 'https://rubygems.org'

gem 'mongoid', '~> 7.0'
gem 'rails',   RUBY_VERSION >= '2.5' ? '~> 6.0' : '~> 5.1'
gem 'i18n',    '~> 0.7'

platforms :jruby do
  gem "activerecord-jdbc-adapter"
  gem "activerecord-jdbcsqlite3-adapter"
  gem "jruby-openssl"
end

platforms :ruby do
  gem "sqlite3", "~> 1.4.2"
end

gemspec path: '../'
